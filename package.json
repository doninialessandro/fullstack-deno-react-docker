{
  "name": "fullstack-deno-react-docker",
  "version": "0.1.0",
  "description": "Fullsact application using Deno and React",
  "private": true,
  "scripts": {
    "install-and-start-client": "cd client && npm install && npm start",
    "start-client": "cd client && npm start",
    "start-server": "cd server && denon start",
    "start-app": "docker-compose up -d --build",
    "stop-app": "docker-compose down",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "commitizen": "^4.2.3"
  }
}
